<launch>

    <!--- Depth image to laser scan -->
    <node pkg="depthimage_to_laserscan" type="depthimage_to_laserscan" name="depth_scan2" >
        <param name="scan_height" value="1"/>
        <param name="range_min" value="0.3"/>
        <param name="range_max" value="10"/>
        <param name="scan_offset" value="0"/> 
        <param name="output_frame_id" value="d435_depth_frame"/>
        <remap from="image" to="/d435/depth/image_rect_raw" />
        <remap from="scan" to="/realsense_scan2" />
    </node>

</launch>
